#Problem: https://leetcode.com/problems/palindrome-number/
# class Solution:
def isPalindrome(self, x: int) -> bool:
    if x < 0:
        return False
    else:
        # for calaulation
        temp = x
        # keep the reverse num
        compare = 0
        # reverse the num
        while(temp >= 1):
            compare = compare*10 + temp % 10
            temp = int(temp / 10)
        print(compare)
        if(compare == x):
            return True
        else:
            return False

print(isPalindrome(0, int(input())))
