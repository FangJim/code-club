#Problem: https://leetcode.com/problems/valid-parentheses/
stack = []
top = -1

def stack_append(stack, item):
    # pass data by sharing
    stack.append(item)


def stack_pop(stack, item):
    stack.pop()


def isEmpty(stack):
    if not stack:  # If stack is empty
        return False
    else:
        return True

def topItem(stack):
    return stack[top]

